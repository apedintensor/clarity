# Google Gemini API key (required for AI features)
# The AI SDK uses GOOGLE_GENERATIVE_AI_API_KEY at runtime
GOOGLE_GENERATIVE_AI_API_KEY=your-gemini-api-key-here

# SQLite database path (optional, defaults to ./clarity.db)
DATABASE_URL=./clarity.db

# Web server port (optional, defaults to 3000)
PORT=3000
